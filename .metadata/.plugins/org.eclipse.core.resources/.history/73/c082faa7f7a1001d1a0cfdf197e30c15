package homework5_4;

import java.io.FileWriter;
import java.io.IOException;

public class Filewrite {

	public static void main(String[] args) {
		try {
			FileWriter fw=new FileWriter("C:\\javagit\\file.Employee.txt",true);
			   fw.write("112313");
			   
			   fw.close();
	}
	
	catch(IOException e) {
		System.out.print(false);
	}
}
}
