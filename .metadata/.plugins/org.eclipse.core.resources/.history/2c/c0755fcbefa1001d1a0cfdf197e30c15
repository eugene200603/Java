package homework5;

public abstract class Employee {
 private String name;
 private String sex;
 private String date;
 private String phone;
 private String email;
 
 


 public Employee(String name,String sex,String date,String phone,String email) {
	 this.name=name;
	 this.sex=sex;
	 this.date=date;
	 this.phone=phone;
	 this.email=email;
 }


 public void printData() {
	
	System.out.print("姓名:"+name);
	System.out.print(",性別:"+sex);
	System.out.print(",到職日:"+date);
	System.out.print(",電話:"+phone);
	System.out.print(",信箱:"+email);
 }


}